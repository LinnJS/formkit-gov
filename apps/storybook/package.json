{
  "name": "@formkit-gov/storybook",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "storybook build",
    "dev": "storybook dev -p 4002 --no-open",
    "dev:open": "open http://localhost:4002 && storybook dev -p 4002 --no-open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "http-server storybook-static -p 4002",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:storybook": "test-storybook --url http://localhost:4002 --excludeTags play-fn --maxWorkers=1",
    "test:storybook:ci": "pnpm build && start-server-and-test preview http://localhost:4002 test:storybook",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@formkit-gov/core": "workspace:*",
    "@formkit-gov/react": "workspace:*"
  },
  "devDependencies": {
    "@department-of-veterans-affairs/component-library": "^54.2.0",
    "@department-of-veterans-affairs/css-library": "^0.27.0",
    "@department-of-veterans-affairs/web-components": "^22.2.0",
    "@formkit-gov/eslint-config": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@playwright/test": "^1.49.1",
    "@standard-schema/spec": "^1.0.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.6.14",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "axe-playwright": "^2.2.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.16.0",
    "http-server": "^14.1.1",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.67.0",
    "start-server-and-test": "^2.1.3",
    "storybook": "^8.6.15",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "wait-on": "^9.0.3",
    "zod": "^4.1.13"
  }
}
