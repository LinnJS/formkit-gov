{
  "name": "@formkit-gov/wizard",
  "version": "0.0.0",
  "description": "Multi-step form orchestration with government form patterns including save-in-progress, review, and confirmation pages",
  "keywords": [
    "forms",
    "wizard",
    "multi-step",
    "stepper",
    "save-in-progress",
    "government",
    "va-forms"
  ],
  "homepage": "https://docs.formkit-gov.org/wizard",
  "bugs": {
    "url": "https://github.com/LinnJS/formkit-gov/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LinnJS/formkit-gov.git",
    "directory": "packages/wizard"
  },
  "license": "MIT",
  "author": "LinnJS",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "dev": "tsup --watch",
    "dev:open": "tsup --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@formkit-gov/core": "workspace:*",
    "@formkit-gov/react": "workspace:*",
    "@formkit-gov/store": "workspace:*"
  },
  "devDependencies": {
    "@department-of-veterans-affairs/component-library": "^54.4.1",
    "@formkit-gov/eslint-config": "workspace:*",
    "@formkit-gov/typescript-config": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.16.0",
    "happy-dom": "^20.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@department-of-veterans-affairs/component-library": ">=50.0.0",
    "@hookform/resolvers": ">=3.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-hook-form": ">=7.45.0",
    "zod": ">=3.20.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
